{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "./dist/out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "lib": [
      "es2017",
      "dom"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "src/admin/**/*.*",
    "coverage",
    "config",
    "aot",
    "**/*.spec.ts",
    "**/*.e2e.ts",
    "docs",
    "typedocs"
  ],
  "compileOnSave": false
}
